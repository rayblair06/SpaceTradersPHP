{
    "name": "rayblair/spacetradersphp",
    "description": "SpacetradersPHP",
    "keywords": [
        "rayblair",
        "SpacetradersPHP"
    ],
    "homepage": "https://github.com/rayblair/spacetradersphp",
    "license": "MIT",
    "authors": [
        {
            "name": "Ray Blair",
            "email": "rayblair06@hotmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "guzzlehttp/guzzle": "^7.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "RayBlair\\SpaceTradersPHP\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RayBlair\\SpaceTradersPHP\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
